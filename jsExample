function onLoad(hi) { //onLoad function
  return hi * hi //returns squared number
}
var x = onLoad() //x is equal to the function value
if (x == 9) { //if statemebnt
    console.log("hello")
  } else {
    console.log("hey")
  };
  //this if statemment depends on the value of x, or the onLoad function
